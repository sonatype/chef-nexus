<%
    nexus_iq_jar = node['nexus-iq']['install_dir'] + "/nexus-iq-server-#{node['nexus-iq']['version']}.jar"
    nexus_iq_config = node['nexus-iq']['conf_dir'] + '/config.yml'
%>

cd <%= node['nexus-iq']['install_dir'] %>
exec /usr/bin/java -jar <%= nexus_iq_jar  %> server <%= nexus_iq_config  %>
